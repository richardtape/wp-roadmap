	"homepage": "https://github.com/johnbillion/extended-cpts/",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "John Blackbourn",
			"homepage": "https://johnblackbourn.com/"
		}
	],
	"require": {
		"php": "^7.0"
	},
	"require-dev": {
		"johnbillion/php-docs-standards": "^1.2",
		"phpunit/phpunit": "^5.7",
		"slevomat/coding-standard": "^4.0",
		"wp-coding-standards/wpcs": "^0.13.0"
	},
	"autoload": {
